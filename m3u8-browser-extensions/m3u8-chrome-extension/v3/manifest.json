{
  // Extension ID: gdcogkdiaaholacgcdafpihelmdjmepe
  "key": "",

  "manifest_version": 3,

  "name"       : "m3u8 file downloader (manifest.v3)",
  "description": "m3u8 file downloader chrome extension (manifest.v3).",
  "version"    : "2.0",
  "offline_enabled": true,
  "icons": {
    "32" : "img/m3u8.png",
    "128": "img/m3u8_148.png"
  },
  "options_ui": {
    "page": "options.html"
  },
  "action": {
    "default_icon" : "img/m3u8.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "storage",
    "scripting",
    "tabs",
    "webRequest",
    "webNavigation",
    "background",
    "notifications",
    "nativeMessaging"
  ],
  "host_permissions": [ "http://*/*", "https://*/*" ],

  "background": {
    "service_worker": "js/serviceWorker.js"
  },

  "content_scripts": [{
    "matches": [ "http://*/*", "https://*/*" ],
    "js"     : [ "js/workInfoType.js" ],
    "run_at" : "document_end"
  }]
}