<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="m3u8.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
    </sectionGroup>
  </configSections>

  <runtime>
    <gcServer               enabled="true" />
    <gcConcurrent           enabled="true" />
    <Thread_UseAllCpuGroups enabled="true" />
    <GCCpuGroup             enabled="true" />
  </runtime>

  <system.net>
    <connectionManagement>
      <add address="*" maxconnection="1024" />
    </connectionManagement>
  </system.net>  

  <userSettings>
    <m3u8.Properties.Settings>
      <setting name="_IsUpgradedInThisVersion" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="OutputFileDirectory" serializeAs="String">
        <value>E:\</value>
      </setting>
      <setting name="OutputFileExtension" serializeAs="String">
        <value>.avi</value>
      </setting>
      <setting name="MaxDegreeOfParallelism" serializeAs="String">
        <value>8</value>
      </setting>
      <setting name="UseCrossAppInstanceDegreeOfParallelism" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="MaxDownloadAppInstance" serializeAs="String">
        <value>4</value>
      </setting>
      <setting name="AutoMinimizeWindowWhenStartsDownload" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="AutoCloseApplicationWhenEndsDownload" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="AttemptRequestCountByPart" serializeAs="String">
        <value>3</value>
      </setting>
      <setting name="RequestTimeoutByPart" serializeAs="String">
        <value>00:00:15</value>
      </setting>
      <setting name="DownloadLogUIType" serializeAs="String">
        <value>GridViewUIType</value>
      </setting>
      <setting name="ShowOnlyRequestRowsWithErrors" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="NameCleanerExcludesWords" serializeAs="Xml">
        <value>
          <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
